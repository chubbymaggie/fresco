      version="@PACKAGE_VERSION@",
      description="Fresco Python bindings",
      url="http://www.fresco.org",
      package_dir = {'' : 'src'},
      packages=["Fresco", "Fresco.Input",
                "Layout", "Layout__POA",
                "Figure", "Figure__POA",
                "Primitive", "Primitive__POA",
                "Widget", "Widget__POA",
                "Unidraw", "Unidraw__POA",
                "FrescoStubs"])
